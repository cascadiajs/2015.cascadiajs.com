
<div class="clearfix speaker <%- typeof classname != 'undefined' ? classname : '' %>">
  <div class="inner">
    <% if(typeof avatar !== "undefined") { %>
    <img alt="" src="<%- avatar %>" class="profile-image"/>
    <% } %>

    <h4><%- name %></h4>
    <% if(typeof talk !== "undefined") { %>
    <p>
      <% if(talk.url) { %>
      <a href="<%- talk.url %>">
      <% } %>

        <%- talk.title %>

      <% if(talk.url) { %>
      </a>
      <% } %>
    </p>
    <% } %>
  </div>
</div>