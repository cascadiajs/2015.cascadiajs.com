        <h2><a href="/css/">Cascadia<span class="thin">CSS</span></a></h2>

        <%
        for(var i=0;i<public._data.speakers.css.length;i++) {
          var speaker = public._data.speakers.css[i];
          %>

          <%- partial("_partials/speaker", speaker) %>
        <%
        }
        %>

